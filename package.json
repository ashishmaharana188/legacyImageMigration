{
  "name": "pdf-processor",
  "version": "1.0.0",
  "description": "PDF processor full-stack application - monorepo",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "install:all": "npm install",
    "dev": "concurrently \"npm run dev --workspace=backend\" \"npm run dev --workspace=frontend\"",
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev:backend": "npm run dev --workspace=backend",
    "build": "npm run build:backend && npm run build:frontend",
    "build:frontend": "npm run build --workspace=frontend",
    "build:backend": "npm run build --workspace=backend",
    "start": "concurrently \"npm run start --workspace=backend\" \"npm run start --workspace=frontend\"",
    "start:prod": "concurrently \"npm run start:prod --workspace=backend\" \"npm run start:prod --workspace=frontend\"",
    "clean": "npm run clean --workspace=backend && npm run clean --workspace=frontend",
    "lint": "npm run lint --workspace=frontend && npm run lint --workspace=backend",
    "test": "npm run test --workspace=frontend && npm run test --workspace=backend"
  },
  "keywords": [
    "pdf",
    "processor",
    "react",
    "node",
    "express",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/ws": "^8.18.1",
    "concurrently": "^9.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
